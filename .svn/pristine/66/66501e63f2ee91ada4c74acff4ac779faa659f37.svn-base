/**
评价轮播图
**/
.text-wrapper{
 
}

  .text {
        padding: 20px 10px;
        display: flex;
        align-items: center;
        background: #fff;
       
    }

    .text .head-wrapper {
        width: 68px;
        text-align: center;
    }

    .text .head-wrapper image {
        width: 50px;
    }

    .text .line {
        width: 1px;
        height: 50px;
        background: #ffd097;
        margin: 5px 10px;
    }

    .text .content {
        flex: 1;
    }

    .text .content view {
        color: #333;
        font-size: 14px;
        line-height: 20px;
    }

    .text .content view:nth-child(2) {
        color: #999;
        font-size: 12px;
        text-align: right;
        margin-top: 5px;
    }
/**
三个板块
**/
.three{
  display:flex;

}
.three>view:first-child{
  width:47.2%;
}
.three>view:nth-child(2){
  flex:1;
}
.three>view>image{
  width:100%;display:block;
}


/**快速下单**/

.fast {
  align-items: center;
  display: flex;
  padding: 20px 0;
}
.fast .fast-icon {
  width: 50px;
}
.fast .fast-icon image {
  width: 50px;
}
.fast .content {
  flex: 1;
  align-items: center;
  padding-left: 10px;
}
.fast .content view {
  color: #999;
  font-size: 12px;
  line-height: 25px;
}
.fast .content view.fast-txt {
  font-size: 18px;
  color: #333;
  font-weight: 600;
}
.fast .arrow {
  transition: all .3s;
}
.fast .arrow image {
  width: 20px;
}
.images-wrapper{
  padding:20px 10px;background: #fff;
}
.images-wrapper image{
  display:block;width:100%;
}
.images-wrapper>view:nth-child(2){
  margin-top:5px;
}



.wrapper > .notice {
  display: flex;
  margin-top: 10px;
  padding: 10px 0;
}
.wrapper > .notice > view {
  flex: 1;
  width: 16px;
  font-size: 12px;
  text-align: center;
}
.wrapper > .notice > view image {
  width: 30px;
}
.wrapper > .notice view {
  margin-top: 10px;
}


.fault-list::after{
  content:"";
  display:table;
  clear:both;
}
.fault-list>view{
  float:left;width:50%;box-sizing: border-box;background:#fff;font-size:14px;border-bottom:2px solid #f7f7f2;
  padding:20px 10px;text-align:center;
}
.fault-list>view:nth-child(2n+1){
  border-right:2px solid #f7f7f2;
}
.fault-list>view>view:first-child{
      height: 20px;
    line-height: 20px;
    font-size: 16px;
    font-weight: bold;
    margin-bottom: 10px;
}
.fault-list>view>view:nth-child(2){
      color: #999;
    font-size: 12px;
    display: block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;

}
.fault-list>view>view:nth-child(3){
      background: #ff8500;
    color: #fff;
    width: 80px;
    height: 22px;
    line-height: 22px;
    margin-top: 10px;
    text-align: center;
    font-size: 14px;
    border-radius: 4px;
    display: inline-block;
  
}


/**
  新闻
**/
   .news {
        padding: 20px 20px 10px 20px;
        background: #fff;margin-top:10px;
    }
    .news .title {
        font-size: 18px;
        font-weight: bold;
    }
    .news .box-new {
        display: flex;
        padding: 20px 0;
      
    }
    .news .box-new .left-image {
        width: 130px;
    }
    .news .box-new .left-image image {
        width: 100%;
    }
    .news .box-new .right-content {
        flex: 1;
        padding: 10px;
    }
    .news .box-new .right-content .new-title {
        font-size: 14px;
        line-height: 20px;
        color: #434343;
        height: 40px;
        overflow: hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
    }
    .news .box-new .right-content .read-num {
        font-size: 12px;
        color: #999;
        margin-top: 10px;
    }