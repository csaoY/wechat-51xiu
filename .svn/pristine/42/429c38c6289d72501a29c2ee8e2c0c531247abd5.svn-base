/* pages/assessment/placeorder/placeorder.wxss */
.con{
font-size:30rpx;
}
.collections{
    width:730rpx;background:white;margin-top:1px;padding:10rpx;
}
.collections-title{
    color:#ff9d00;margin-top:30rpx;
}
.hr{
    width:730rpx;height:5rpx;border-radius: 1rpx;background:#eee;margin-top:10rpx;
}
.hr-left{
     width:20%;height:5rpx;background:#ff9d00
}
.message{
    color:#999;margin-top:15rpx;
}
.message>text{color:#ff9d00;}
.hr1{
    width:730rpx;margin:0 auto;
}
input{
    height:100rpx;margin-top:10rpx;border:1px solid #eee;width:718rpx;font-size:13px;padding-left:10rpx;
}
textarea{
    margin-top:10rpx;border:1px solid #eee;width:718rpx;font-size:13px;padding-left:10rpx;
}
.price{
    margin-top:20rpx;position:relative;margin-bottom:60rpx;
}
.pri{
    position:absolute;right:10rpx;color:#ff9d00
}
.all-pri{
    float:right;margin-right:10rpx;
}
.all-pri>text{
    color:#ff9d00
}
.xiu{
    color:#ff9d00;
}

.section-all{
    display: flex;flex-direction: row;
}
.section{
border:1rpx solid #eee;width:340rpx;height:80rpx;text-align: center;line-height:80rpx;margin-top:10rpx;
}
.section:nth-child(1){
margin-right:40rpx;
}
.ico{
    width:8rpx;height:8rpx;border:1px solid #d3d3d3;border-radius:4rpx;display:inline-block
}
.list{
    min-height:100rpx;
}
.list>view{
    display:inline-block;
}
.list-tit{width:250rpx;float:left;font-size:24rpx;

}
.list-con{width:280rpx;float:left;margin-left:20rpx;font-size:24rpx;


}
.list-pri{
    position:absolute;right:10rpx;color:#ff9d00
}

.hr1{
    width:100%;height:1px;background:#d3d3d3;margin:5rpx 0

}
.last-view{
    margin:10rpx 0;
}
.last-price{
    color:#ff9d00
}
.bot {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  padding: 16rpx 0;
  border-top: 1px solid #eee;
  background: white;
}
.box{
    height:40rpx;
}

.bot>button {
  width: 97%;
  background: #ff9d00;
  color: #fff;
}
.box{
    height:115rpx;
}